generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model users {
  student_id   Int     @id
  student_name String? @db.VarChar(50)
  gender       String? @db.VarChar(10)
  age          Int?
  teacher      String? @db.VarChar(50)
}
